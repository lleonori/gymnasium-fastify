{
  "name": "gymnasium-fastify",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --import tsx/esm --watch src src/index.ts",
    "build": "tsc",
    "postbuild": "cp package*.json dist/ && cp postgrator-config.js dist/ && cp .env.production dist/ && cp -rf migrations dist/",
    "pm2:start": "NODE_ENV=production pm2 start src/index.js --name gymnasium",
    "db:up": "docker compose --env-file .env.development up -d",
    "db:migrate": "postgrator --config postgrator-config.js",
    "db:down": "docker compose down",
    "generate:db:types": "kysely-codegen --env-file=.env.development",
    "pg-pull": "docker pull postgres:$(grep POSTGRES_VERSION .env | cut -d '=' -f2)-alpine",
    "pretest": "npm run pg-pull",
    "test": "vitest",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^5.0.2",
    "@fastify/autoload": "^6.2.0",
    "@fastify/cors": "^10.0.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.1",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@sinclair/typebox": "^0.33.22",
    "change-case": "^5.4.4",
    "desm": "^1.3.1",
    "env-schema": "^6.0.0",
    "fastify": "^5.2.2",
    "fastify-auth0-verify": "^3.0.0",
    "fastify-plugin": "^5.0.1",
    "fs-extra": "^11.2.0",
    "kysely": "^0.27.5",
    "pg": "^8.13.1",
    "qs": "^6.13.1",
    "rambda": "^9.4.0"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.32",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "@types/qs": "^6.9.17",
    "@vitest/coverage-v8": "^2.1.9",
    "close-with-grace": "^2.1.0",
    "dockerode": "^4.0.4",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "kysely-codegen": "^0.17.0",
    "lefthook": "^1.11.7",
    "pino-pretty": "^13.0.0",
    "postgrator-cli": "^9.0.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.28.0",
    "vitest": "^2.1.9"
  }
}
